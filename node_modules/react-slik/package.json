{
  "name": "react-slik",
  "version": "1.0.1",
  "description": "React higher order component for Slik animations",
  "main": "src/index.js",
  "scripts": {
    "start": "http-server examples/ -c-0 -o",
    "watch": "mkdir -p examples/build/js && watchify -t babelify examples/src/js/index.js -o examples/build/js/index.js -v",
    "build": "mkdir -p examples/build/js && browserify -t babelify examples/src/js/index.js -o examples/build/js/index.js",
    "lint": "eslint src/ examples/src/js/",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JakeSidSmith/react-slik.git"
  },
  "keywords": [
    "animation",
    "tween",
    "flow",
    "slik",
    "react",
    "smooth",
    "library",
    "html5",
    "canvas"
  ],
  "author": "Jake 'Sid' Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JakeSidSmith/react-slik/issues"
  },
  "homepage": "https://github.com/JakeSidSmith/react-slik",
  "contributors": [
    {
      "name": "Jake 'Sid' Smith",
      "url": "https://githib.com/JakeSidSmith"
    }
  ],
  "dependencies": {
    "create-react-class": ">=15",
    "react": ">=15",
    "slik": ">=1.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "14.1.0",
    "create-react-class": "15.6.0",
    "eslint-config-jakesidsmith": "git+https://github.com/JakeSidSmith/eslint-config-jakesidsmith.git#v2.1.1",
    "http-server": "0.9.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "slik": "1.2.2",
    "watchify": "3.9.0"
  }
}
